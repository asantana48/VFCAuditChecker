<Window x:Class="VFCApplication.DeviceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VFCApplication"
        Loaded="Window_Loaded"
        Closing="Window_Closing"
        mc:Ignorable="d"
        Title="VFC Audit Checker" Height="600" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="DeviceList" HorizontalAlignment="Left" Height="530" Margin="0,39,0,0" VerticalAlignment="Top" Width="260" AutomationProperties.Name="DeviceList"/>
        <Calendar HorizontalAlignment="Left" Margin="399,74,0,0" VerticalAlignment="Top" Height="160" Width="178"/>
        <TextBlock HorizontalAlignment="Left" Margin="0,10,0,0" TextWrapping="Wrap" Text="Device List" VerticalAlignment="Top" Height="24" Width="260" FontSize="18" TextAlignment="Center"/>

    </Grid>
</Window>
